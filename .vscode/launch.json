  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Java Collection Application",
      "request": "launch",
      "mainClass": "com.java.developer.javacollection.JavaCollectionApplication",
      "projectName": "java-collection",
      "args": "--spring.profiles.active=localhost",
      "env": {
        "profile": "dev",
        "datasource-username": "postgres",
        "datasource-password": "postgres",
        "datasource-url": "localhost:5432",
        "datasource-db": "import-export-excel-service-2",
        "ddl-auto": "update",
        "show-sql": false,
        "generate-ddl": false,
        "batch-size": 1000,
        "swagger-ui": true,
        "api-docs": true,
        "server-port": 9000
      }
    }
  ]
}